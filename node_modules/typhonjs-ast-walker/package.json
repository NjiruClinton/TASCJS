  },
  "maintainers": [
    {
      "name": "Mike Leahy",
      "email": "support@typhonjs.io",
      "url": "https://github.com/typhonrt"
    }
  ],
  "devDependencies": {
    "typhonjs-config-eslint": "^0.4.0",
    "typhonjs-npm-build-test": "^0.3.0"
